{
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint ./App.js src/",
    "lint-fix": "eslint src/ --fix"
  },
  "dependencies": {
    "@expo/vector-icons": "9.0.0",
    "apisauce": "1.0.3",
    "expo": "33.0.0",
    "expo-constants": "5.0.1",
    "expo-linear-gradient": "5.0.1",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-dotenv": "0.2.0",
    "react-native-paper": "2.16.0",
    "react-native-web": "0.11.4",
    "react-navigation": "3.11.1",
    "react-redux": "7.1.0",
    "redux": "4.0.4",
    "redux-saga": "1.0.5",
    "reduxsauce": "1.1.0",
    "rn-placeholder": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "babel-preset-expo": "5.2.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "jest-expo": "33.0.2",
    "react-hooks-testing-library": "0.6.0",
    "react-native-testing-library": "1.11.1",
    "redux-mock-store": "1.5.3"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverageFrom": [
      "<rootDir>/App.js",
      "<rootDir>/src/**/*.{js,jsx,mjs}",
      "!<rootDir>/src/**/index.js",
      "!<rootDir>/src/features/**/styles/*",
      "!<rootDir>/src/components/**/styles/*",
      "!<rootDir>/src/components/wrappers/styles/*"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/config/*"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|expo|@expo|react-navigation)/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 60,
        "lines": 75,
        "statements": 75
      }
    }
  }
}
