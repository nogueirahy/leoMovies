{
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "apisauce": "1.0.3",
    "expo": "33.0.0",
    "moment": "2.24.0",
    "react": "16.8.3",
    "react-dom": "16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-paper": "2.16.0",
    "react-native-web": "0.11.4",
    "react-navigation": "3.11.1",
    "react-redux": "7.1.0",
    "redux": "4.0.4",
    "redux-saga": "1.0.5",
    "reduxsauce": "1.1.0"
  },
  "devDependencies": {
    "babel-preset-expo": "5.1.1",
    "jest-expo": "33.0.2",
    "react-hooks-testing-library": "0.6.0",
    "react-native-dotenv": "0.2.0",
    "react-native-testing-library": "1.11.1",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverageFrom": [
      "<rootDir>/App.js",
      "<rootDir>/src/**/*.{js,jsx,mjs}",
      "!<rootDir>/src/**/index.js",
      "!<rootDir>/src/features/**/styles/*",
      "!<rootDir>/src/components/**/styles/*",
      "!<rootDir>/src/components/wrappers/styles/*"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/config/*"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|expo|@expo|react-navigation)/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
